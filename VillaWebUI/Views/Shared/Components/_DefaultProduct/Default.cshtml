@model List<ResultProductDto>
<div class="properties section" id = "properties">
    <div class="container">
        <div class="row">
            <div class="col-lg-4 offset-lg-4">
              
                    <div class="section-heading text-center">
                        
                        <h2>Mülklerimiz</h2>
                    </div>
              
            </div>
        </div>
        <div class="row">
            @foreach (var item in Model)
            {

                <div class="col-lg-4 col-md-6">

                    <div class="item">
                        <a href="property-details.html"><img src="@item.ImageUrl" alt=""></a>
                        <span class="category">@item.Category</span>
                        <h6>@item.Price</h6>
                        <h4><a href="property-details.html">18 New Street Miami, OR 97219</a></h4>
                        <ul>
                            <li>Bedrooms: <span>@item.BedroomCount</span></li>
                            <li>Bathrooms: <span>@item.BathroomCount</span></li>
                            <li>Area: <span>@item.Area</span></li>
                            <li>Floor: <span>@item.Floor</span></li>
                            <li>Parking: <span>@item.ParkingCount</span></li>
                        </ul>
                        <div class="main-button">
                            <a href="property-details.html">Ziyaret Et</a>
                        </div>
                    </div>

                </div>
            }
        
        
        </div>
    </div>
</div>
