@model List<ResultContactDto>

<div class="contact section" id="contact">
    <div class="container">
        <div class="row">
            <div class="col-lg-4 offset-lg-4">
                <div class="section-heading text-center">
                    <h6>| İletişim Bilgilerimiz</h6>
                    <h2>
                        Temsilcilerimizle İletişime Geçin
                    </h2>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="contact-content">
    <div class="container">
        <div class="row">
            @foreach (var item in Model)
            {
                <div class="col-lg-7">
                    <div id="map">
                        <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3029.4535870232776!2d29.170548880446034!3d40.59781501015745!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x14caf6cfb8550a5b%3A0x982bacec0eb79274!2s%C3%9Cvezp%C4%B1nar%2C%20Cami%20Sk.%2C%2077400%20Termal%2FYalova!5e0!3m2!1str!2str!4v1724494659563!5m2!1str!2str" width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
                    </div>
                    <div class="row">
                        <div class="col-lg-6">
                            <div class="item phone">
                                <img src="~/VillaAgency-1.0.0/assets/images/phone-icon.png" alt="" style="max-width: 52px;">
                                <h6>@item.Phone<br><span>Telefon Numarası</span></h6>
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="item email">
                                <img src="~/VillaAgency-1.0.0/assets/images/email-icon.png" alt="" style="max-width: 52px;">
                                <h6>@item.Email<br><span>Email</span></h6>
                            </div>
                        </div>
                    </div>
                </div>
            }
            
           @await Component.InvokeAsync("_DefaultMessage")
           
        </div>
    </div>
</div>