


<!DOCTYPE html>

<html lang="en">

     @await Component.InvokeAsync("_AdminHead") 
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>Focus - Bootstrap Admin Dashboard </title>

    <link rel="icon" type="image/png" sizes="16x16" href="~/focus-2/images/favicon.png">
    <link href="~/focus-2/css/style.css" rel="stylesheet">

</head>

<body>

  
   @await Component.InvokeAsync("AdminPreLoader")

    <div id="main-wrapper">

       @await Component.InvokeAsync("_AdminLogo");
        
      
        @await Component.InvokeAsync("_AdminNavbar");

       @await Component.InvokeAsync("_AdminSidebarMenu")

        <div class="content-body">
            <div class="container-fluid">
                <div class="row page-titles mx-0">
                    <div class="col-sm-6 p-md-0">
                        <div class="welcome-text">
                            <h4>@ViewData["Title"]</h4>
                        </div>
                    </div>
               
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <div class="card">
                           
                            <div class="card-body">
                              @RenderBody()
                            </div>
                          
                        </div>
                    </div>
                </div>
            </div>
        </div>

        @await Component.InvokeAsync("_AdminFooter");
        


    </div>

  @await Component.InvokeAsync("_AdminScripts");


</body>

</html>

